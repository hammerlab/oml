<*.cmx> and not <oml_lite.cm*> : for-pack(Oml_lite)

# For the sub-packed modules
<util/util.cm*> : for-pack(Oml_lite)

<unc/*.cmx> and not <unc/uncategorized.cm*> : for-pack(Oml_lite.Uncategorized)
<unc/*.cmx> and not <unc/uncategorized.cm*> : for-pack(Uncategorized)
<unc/uncategorized.cm*> : for-pack(Oml_lite)

<stats/*.cmx> and not <stats/statistics.cm*> : for-pack(Oml_lite.Statistics)
<stats/*.cmx> and not <stats/statistics.cm*> : for-pack(Statistics)
<stats/statistics.cm*> : for-pack(Oml_lite)

<cls/*.cmx> and not <cls/classification.cm*> : for-pack(Oml_lite.Classification)
<cls/*.cmx> and not <cls/classification.cm*> : for-pack(Classification)
<cls/classification.cm*> : for-pack(Oml_lite)

<rgr/*.cmx> and not <rgr/regression.cm*> : for-pack(Oml_lite.Regression)
<rgr/*.cmx> and not <rgr/regression.cm*> : for-pack(Regression)
<rgr/regression.cm*> : for-pack(Oml_lite)

<uns/*.cmx> and not <uns/unsupervised.cm*> : for-pack(Oml_lite.Unsupervised)
<uns/*.cmx> and not <uns/unsupervised.cm*> : for-pack(Unsupervised)
<uns/unsupervised.cm*> : for-pack(Oml_lite)
