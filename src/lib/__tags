<*.cmx> and not <oml.cm*> : for-pack(Oml), for-pack(Oml-lite)

# For the sub-packed modules
<util/util.cm*> : for-pack(Oml), for-pack(Oml-lite)

<unc/*.cmx> and not <unc/uncategorized.cm*> : for-pack(Oml.Uncategorized)
<unc/*.cmx> and not <unc/uncategorized.cm*> : for-pack(Uncategorized)
<unc/uncategorized.cm*> : for-pack(Oml)

<stats/*.cmx> and not <stats/statistics.cm*> : for-pack(Oml.Statistics)
<stats/*.cmx> and not <stats/statistics.cm*> : for-pack(Statistics)
<stats/statistics.cm*> : for-pack(Oml)

<cls/*.cmx> and not <cls/classification.cm*> : for-pack(Oml.Classification)
<cls/*.cmx> and not <cls/classification.cm*> : for-pack(Classification)
<cls/classification.cm*> : for-pack(Oml)

<rgr/*.cmx> and not <rgr/regression.cm*> : for-pack(Oml.Regression)
<rgr/*.cmx> and not <rgr/regression.cm*> : for-pack(Regression)
<rgr/regression.cm*> : for-pack(Oml)

<uns/*.cmx> and not <uns/unsupervised.cm*> : for-pack(Oml.Unsupervised)
<uns/*.cmx> and not <uns/unsupervised.cm*> : for-pack(Unsupervised)
<uns/unsupervised.cm*> : for-pack(Oml)
